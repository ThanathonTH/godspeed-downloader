{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Godspeed Downloader",
  "version": "2.1.1",
  "identifier": "com.godspeed.downloader",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "godspeed-downloader",
        "width": 800,
        "height": 600,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/godspeed.ico"
    ],
    "externalBin": [
      "yt-dlp",
      "aria2c",
      "ffmpeg"
    ]
  }
}